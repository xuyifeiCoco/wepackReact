{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": [0],
    "react/jsx-filename-extension": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "react/require-default-props": [0],
    "arrow-body-style": [0],
    "no-param-reassign": [0],
    "import/no-unresolved":[0],
    "import/extensions":[0],
    " react/jsx-indent":[0],
    "no-unused-expressions":[0],
    "no-sequences":[0],
    "react/jsx-indent":[0],
    "comma-dangle":[0],
    "no-undef":[0],           
    "react/destructuring-assignment":[0],                  
    "react/prop-types":[0],           
    "react/no-typos":[0],
    "comma-spacing":[0],
    "no-console":[0],
    "import/no-named-as-default":[0],
    "class-methods-use-this":[0],
    "no-redeclare":[0],
    "no-multiple-empty-lines":[0],
    "no-trailing-spaces":[0],
    "block-spacing":[0],
    "no-underscore-dangle":[0],
    "react/forbid-prop-types":[0],
    "react/no-children-prop":[0],
    "max-len":[0],
    "react/no-danger":[0],
    "no-alert":[0]
  }
}
